{
    "name": "llog-server",
    "version": "1.0.0",
    "description": "LLog 本地数据服务器",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "pm2:start": "pm2 start ecosystem.config.js",
      "pm2:stop": "pm2 stop llog-server",
      "pm2:restart": "pm2 restart llog-server",
      "pm2:logs": "pm2 logs llog-server",
      "pm2:monit": "pm2 monit",
      "install:pm2": "npm install -g pm2",
      "install:nodemon": "npm install -g nodemon",
      "setup": "npm install && npm run install:pm2",
      "test": "node test-server.js"
    },
    "keywords": ["llog", "timeline", "local-server", "data-storage"],
    "author": "LLog User",
    "license": "MIT",
    "dependencies": {
      "express": "^4.18.2",
      "cors": "^2.8.5"
    },
    "devDependencies": {
      "nodemon": "^3.0.1"
    },
    "engines": {
      "node": ">=14.0.0"
    }
  }